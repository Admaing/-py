
<view class="container">
	<view class="header">
		<image class="avatar" src="{{userInfo.avatarUrl}}" />
		<view class="nickname">{{userInfo.nickName}}</view>
	</view>
	<view class="section">
		<view class="line judgement" catchtap='me_issue'>我的发布</view>
		<view class="line judgement" catchtap='me_order'>我的接单</view>
	<!--<view class="line judgement">我的收藏</view>-->
		<view class="line judgement" catchtap='address'>我的地址</view>
	</view>
	<view class="section">
		<view class="line judgement">我的钱包<text class="tl">余额：￥0</text></view>
	</view>
	<view class="section">
	
		<view class="line judgement">商家入驻</view>
	</view>
	<view class="section">
	
		<view class="line judgement"><button open-type="contact">在线客服</button><text class="tl"></text></view>
    	<view class="line judgement" catchtap='onShareAppMessage'><button open-type="share">分享给朋友</button></view>
	</view>
</view>
